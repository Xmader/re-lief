if (sf.gallary_init === void){
	// 最初に一度だけ実行
	// 見たかどうかのフラグ
	sf.ai_cg_flag = %[];
	for (var i = 0; i < ai.cg_storage.count; i++){
		if (typeof(ai.cg_storage[i]) == 'Object'){
			sf.ai_cg_flag[i] = %[];
				for (var n = 0; n < ai.cg_storage[i].count; n++){
					sf.ai_cg_flag[i][n] = false;
				}
		}else{
			sf.ai_cg_flag[i] = false;
		}
	}
	sf.gallary_init = 1;
}
ai.complete = function (){ //全CGを見たことにする(デバッグ用)
	for (var i=0; i < ai.cg_storage.count; i++){
		if (typeof(ai.cg_storage[i]) == 'Object'){
			for (var n=0; n < ai.cg_storage[i].count; n++){
				sf.ai_cg_flag[i][n]=true;
			}
		}else{
			sf.ai_cg_flag[i] = true;
		}
	}
} incontextof global;
ai.modecount = function (num){ //差分画像の見た割合を返す
	var saw = 0;
	for (var i=0; i < ai.cg_storage[num].count; i++){
		if (sf.ai_cg_flag[num][i])
			saw+=1;
	}
	return saw + '/' + ai.cg_storage[num].count;
} incontextof global;
function flagai(elm){
	var i = ai.cg_storage.find("cg/"+elm.src);
	if (i == -1){
	var n;
		for ( n = 0; n < ai.cg_storage.count; n++){
			if (typeof(ai.cg_storage[n]) == 'Object'){
				if (ai.cg_storage[n].find("cg/"+elm.src) >= 0){
					i = ai.cg_storage[n].find("cg/"+elm.src);
					break;
				}
			}
		}
		if (n != ai.cg_storage.count)
			sf.ai_cg_flag[n][i] = true;
	}else{
		sf.ai_cg_flag[i] = true;
	}
}
ai.wheel = function (shift, delta, x, y) {
	return;
} incontextof global;

