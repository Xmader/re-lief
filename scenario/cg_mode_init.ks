if (sf.gallary_init === void){
	// 最初に一度だけ実行
	// 見たかどうかのフラグ
	sf.bg_cg_flag = %[];
	for (var i = 0; i < bg.cg_storage.count; i++){
		if (typeof(bg.cg_storage[i]) == 'Object'){
			sf.bg_cg_flag[i] = %[];
				for (var n = 0; n < bg.cg_storage[i].count; n++){
					sf.bg_cg_flag[i][n] = false;
				}
		}else{
			sf.bg_cg_flag[i] = false;
		}
	}
	sf.gallary_init = 1;
}
bg.complete = function (){ //全CGを見たことにする(デバッグ用)
	for (var i=0; i < bg.cg_storage.count; i++){
		if (typeof(bg.cg_storage[i]) == 'Object'){
			for (var n=0; n < bg.cg_storage[i].count; n++){
				sf.bg_cg_flag[i][n]=true;
			}
		}else{
			sf.bg_cg_flag[i] = true;
		}
	}
} incontextof global;
bg.modecount = function (num){ //差分画像の見た割合を返す
	var saw = 0;
	for (var i=0; i < bg.cg_storage[num].count; i++){
		if (sf.bg_cg_flag[num][i])
			saw+=1;
	}
	return saw + '/' + bg.cg_storage[num].count;
} incontextof global;
function flagbg(elm){
	var i = bg.cg_storage.find("bg/"+elm.src);
	if (i == -1){
	var n;
		for ( n = 0; n < bg.cg_storage.count; n++){
			if (typeof(bg.cg_storage[n]) == 'Object'){
				if (bg.cg_storage[n].find("bg/"+elm.src) >= 0){
					i = bg.cg_storage[n].find("bg/"+elm.src);
					break;
				}
			}
		}
		if (n != bg.cg_storage.count)
			sf.bg_cg_flag[n][i] = true;
	}else{
		sf.bg_cg_flag[i] = true;
	}
}
bg.wheel = function (shift, delta, x, y) {
	return;
} incontextof global;

if (sf.gallary_bg_init === void){
	sf.bg_cg_flag = %[];
	for (var i = 0; i < bg.cg_storage.count; i++){
		if (typeof(bg.cg_storage[i]) == 'Object'){
			sf.bg_cg_flag[i] = %[];
				for (var n = 0; n < bg.cg_storage[i].count; n++){
					sf.bg_cg_flag[i][n] = false;
				}
		}else{
			sf.bg_cg_flag[i] = false;
		}
	}
	sf.gallary_bg_init = 1;
}
bg.page = 0;
bg.maxpage = bg.cg_sstorage.count%(bg.column*bg.line) == 0 ? bg.cg_sstorage.count\(bg.column*bg.line) - 1 : bg.cg_sstorage.count\(bg.column*bg.line);


