@iscript
if(sf.font_size === 1){
	tf.len= tf.text.length;
  if(tf.len>28){
    tf.len2 = tf.len-28;
    if(tf.len2>28){
      tf.len3 =tf.len2-28;
    }
  }
}
else{
	tf.len= tf.text.length;
  	if(tf.len>34){
    	tf.len2 = tf.len-34;
    	if(tf.len2>34){
      		tf.len3 =tf.len2-34;
    	}
  	}
 }
@endscript
@return
